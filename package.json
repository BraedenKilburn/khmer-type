    "eslint": "^9.28.0",
    "eslint-plugin-oxlint": "^0.17.0",
    "eslint-plugin-vue": "~10.1.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^0.17.0",
    "prettier": "3.5.3",
